
@model List<Models.ChartData>

@{
	Layout = null;
}

@foreach (var chart in Model)
{
	<div class="col-md-12 chartPreview" id="@chart.ObservedDataId">
		<iframe src="@Url.Action("GetChart","Profile", new {dataSetName = chart.ObservedDataId}, this.Request.Url.Scheme)" style="border: 0;  width:100%; height:100%" class="chartPreview"></iframe>
	</div>
}